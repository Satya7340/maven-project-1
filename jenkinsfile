pipeline
{
agent any
stages
{
    stage('scm checkout') 
    {
    steps { git branch: 'master', url: 'https://github.com/prakashk0301/maven-project' }
    }

    stage ('code build')
    {
        steps { withMaven(jdk: 'my_Java', maven: 'my_maven') 
        {  sh 'mvn clean package'  } 
              }
    }

}

}
